{
  "schemaVersion": 1,
  "id": "chatdebug",
  "version": "${version}",
  "name": "ChatDebug",
  "description": "lets you view the chat messages as JSON / raw ChatComponent code",
  "authors": ["TriPledOp"],
  "contact": {},
  "license": "MIT",
  "icon": "assets/chatdebug/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "dev.tripledop.chatdebug.client.ChatDebugModClient"
    ],
    "main": [
      "dev.tripledop.chatdebug.ChatDebugMod"
    ],
    "modmenu": [
      "dev.tripledop.chatdebug.client.ModMenuIntegration"
    ]
  },
  "mixins": [
    "chatdebug.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "cloth-config": ">=18.0.145"
  }
}
